{
    "name": "joseph-bing-han/laravel-admin",
    "replace": {
        "dcat/laravel-admin": "2.2.2-beta"
    },
    "description": "Enhanced Dcat Admin with localized time format support and minor adjustments for improved usability.",
    "type": "library",
    "keywords": [
        "dcat",
        "laravel",
        "admin",
        "grid",
        "form",
        "laravel admin"
    ],
    "homepage": "https://github.com/joseph-bing-han/dcat-admin",
    "license": "MIT",
    "authors": [
        {
            "name": "Joseph",
            "email": "joseph.bing.han@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "~5.5|~6.0|~7.0|~8.0|~9.0|~10.0",
        "spatie/eloquent-sortable": "3.*|4.*",
        "doctrine/dbal": "^2.6|^3.0"
    },
    "require-dev": {
        "laravel/dusk": "~5.9|~6",
        "phpstan/phpstan": "^0.12.0",
        "phpunit/phpunit": "^7.5|~9",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Dcat\\Admin\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Dcat\\Admin\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dcat\\Admin\\AdminServiceProvider"
            ]
        }
    }
}